{
  "name": "@hackclub/scrappy-bot",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "node server.js",
    "start": "NODE_ENV=production yarn build && node dist/bin/www",
    "build": "rimraf dist/ && babel ./ --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log --copy-files",
    "format": "prettier --write .",
    "checkFormat": "prettier --check ."
  },
  "dependencies": {
    "@mux/mux-node": "^2.8.0",
    "airtable-plus": "^1.0.4",
    "bottleneck": "^2.19.5",
    "cheerio": "^1.0.0-rc.3",
    "express": "^4.17.1",
    "form-data": "^3.0.0",
    "heic-convert": "^1.2.3",
    "lodash": "^4.17.19",
    "node-emoji": "^1.10.0",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.0",
    "@babel/core": "^7.13.1",
    "@babel/plugin-proposal-optional-chaining": "^7.13.0",
    "@babel/plugin-transform-runtime": "^7.13.6",
    "@babel/preset-env": "^7.13.5",
    "dotenv": "^8.2.0",
    "nodemon": "^2.0.4",
    "prettier": "2.0.5",
    "rimraf": "^3.0.2"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-proposal-optional-chaining",
      "@babel/plugin-transform-runtime"
    ]
  },
  "cacheDirectories": [
    ".next/cache"
  ]
}
